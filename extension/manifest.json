{
    "name": "Native Token WebExtension",
    "version": "0.0.22",
    "minimum_chrome_version": "50.0",
    "manifest_version": 2,
    "description": "Use your eID smart card on the web",
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [{
        "matches": ["https://*/*", "file:///*"],
        "js": ["content.js"],
        "run_at": "document_end",
        "all_frames": true
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "options_page": "options.html",
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "permissions": ["nativeMessaging"],
    "applications": {
        "gecko": {
            "id": "{A8C21BB9-15D4-4AF0-9BF2-6E1B70FE8C3D}"
        }
    }
}
