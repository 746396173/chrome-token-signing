{
    "name": "Token signing",
    "version": "0.0.22",
    "minimum_chrome_version": "50.0",
    "manifest_version": 2,
    "description": "Use your eID smart card on the web",
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [{
        "matches": ["https://*/*", "file:///*"],
        "js": ["content.js"],
        "run_at": "document_end"
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "permissions": ["nativeMessaging"]
}